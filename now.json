{
  "version": 2,
  "public": false,
  "name": "animations",
  "alias": ["animations"],
  "regions": ["bru1"],
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    {"src": "/service-worker.js", "headers": { "cache-control": "s-maxage=0" } },
    { "handle": "filesystem" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
